        "@types/react": "16.8.19",
        "@types/webpack": "4.4.32",
        "async-wait-until": "^1.2.4",
        "awesome-typescript-loader": "5.2.1",
        "basscss": "8.1.0",
        "basscss-addons": "1.0.0",
        "basscss-basic": "1.0.0",
        "bootstrap": "^4.3.1",
